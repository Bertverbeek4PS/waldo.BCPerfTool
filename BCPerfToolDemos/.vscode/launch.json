{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "LocalDocker",
            "type": "al",
            "request": "launch",
            "environmentType": "OnPrem",
            "server": "http://bccurrent",
            "serverInstance": "BC",
            "authentication": "UserPassword",
